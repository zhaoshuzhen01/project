<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomTabLayout">
        <attr name="CustomIndcatorHeight" format="dimension" />
        <attr name="CustomIndcatorWidth" format="dimension" />
        <attr name="CustomIndicatorColor" format="color" />
        <attr name="CustomAnimatedIndicator" format="enum">
            <enum name="dachshund" value="0" />
            <enum name="pointMove" value="1" />
            <enum name="lineMove" value="2" />
            <enum name="pointFade" value="3" />
            <enum name="lineFade" value="4" />
        </attr>
    </declare-styleable>

</resources>
